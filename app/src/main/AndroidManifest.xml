<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.arjun_mu.ipaytest">
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SuccessActivity" />
        <activity android:name=".FailureActivity" />
        <activity android:name=".CanceledActivity"></activity>

        <activity android:label="Pay via Carrier" android:name="in.verse.mpayment.PaymentActivity" android:configChanges="orientation|screenLayout|screenSize">
    </activity>"
    </application>

</manifest>